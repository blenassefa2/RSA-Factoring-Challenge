#!/usr/bin/python3
import sys
from math import sqrt


arg1 = str(sys.argv[1])

File = open(arg1, 'r')

lines = File.readlines()

for line in lines:
    
    num = int(line)
    a = int(sqrt(num))
    if num % 2 == 0:
        print("{:d}=2*{:d}".format(num, num//2))
        continue

    if a % 2 == 0:
        a += 1
    
    for i in range(3, a + 1, 2):
        if num % i == 0:
            print("{:d}={:d}*{:d}".format(num, num//i, i))
            break
    if num % i != 0:
        print("{:d}=1*{:d}".format(num, num))
